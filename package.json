{
  "name": "fnv-hash",
  "version": "1.1.3",
  "description": "FNV-1a non-cryptographic hash function with node-addon-api",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jumballaya/fnv-hash.git"
  },
  "author": {
    "name": "Patrick Burris",
    "url": "https://www.pburris.me"
  },
  "engines": {
    "node": ">=10"
  },
  "main": "./index.js",
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "scripts": {
    "test": "node-pre-gyp install --build-from-source install && ava && tsd",
    "preinstall": "npm install node-pre-gyp",
    "install": "node-pre-gyp install --fallback-to-build",
    "build": "node-pre-gyp package --release",
    "publish:gyp": "npm run build && node-pre-gyp-github publish --release"
  },
  "keywords": [],
  "devDependencies": {
    "ava": "^3.0.0",
    "node-pre-gyp-github": "^1.4.3",
    "tsd": "^0.11.0"
  },
  "dependencies": {
    "node-addon-api": "^2.0.0",
    "node-pre-gyp": "^0.14.0"
  },
  "ava": {
    "files": [
      "test.js"
    ],
    "source": [
      "index.js"
    ]
  },
  "binary": {
    "module_name": "fnv1a",
    "module_path": "./lib/binding/{configuration}/{node_abi}-{platform}-{arch}/",
    "package_name": "{module_name}-v{version}-{node_abi}-{platform}-{arch}.tar.gz",
    "host": "https://github.com/jumballaya/fnv-hash/releases/download/",
    "remote_path": "v{version}"
  }
}
